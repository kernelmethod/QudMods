<?xml version="1.0" encoding="utf-8" ?>
<embarkmodules>
  <module Class="XRL.CharacterBuilds.Qud.Kernelmethod_ChooseYourFighter_PlayerModelModule">
    <window ID="Chargen/Kernelmethod_ChooseYourFighter_PlayerModel" Prefab="VertScroll" Class="XRL.CharacterBuilds.Qud.UI.Kernelmethod_ChooseYourFighter_PlayerModelWindow">
      <name>Customize</name>
      <title>:customize character:</title>
    </window>

    <!--
      Pregens come from EmbarkModules.xml
      Castes/callings come from Subtypes.xml
    -->
    <models>
      <!-- True Kin pregens -->
      <model Id="Praetorian Prime" Name="{{M|Praetorian Prime}}">
        <tile Path="creatures/sw_praetorian_prime.bmp" Foreground="y" DetailColor="c" />
      </model>
      <model Id="First Gardener" Name="{{M|First Gardener}}">
        <tile Path="creatures/sw_first_horticulturalist.bmp" Foreground="y" DetailColor="g" />
      </model>
      <model Id="First Child of the Hearth" Name="{{M|First Child of the Hearth}}">
        <tile Path="creatures/sw_first_child_of_the_hearth.bmp" Foreground="y" DetailColor="r" />
      </model>

      <!-- Mutant pregens -->
      <model Id="Marsh Taur" Name="{{M|Marsh Taur}}">
        <tile Path="Creatures/sw_marsh_taur.bmp" Foreground="y" DetailColor="g" />
      </model>
      <model Id="Dream Tortoise" Name="{{M|Dream Tortoise}}">
        <tile Path="Creatures/sw_dream_tortoise.bmp" Foreground="y" DetailColor="w" />
      </model>
      <model Id="Gunwing" Name="{{M|Gunwing}}">
        <tile Path="Creatures/sw_gunwing.bmp" Foreground="y" DetailColor="K" />
      </model>
      <model Id="Star-Eye Esper" Name="{{M|Star-Eye Esper}}">
        <tile Path="creatures/sw_stareye_esper.bmp" Foreground="y" DetailColor="m" />
      </model>
      <model Id="Firefrond" Name="{{M|Firefrond}}">
        <tile Path="Creatures/sw_firefrond.bmp" Foreground="y" DetailColor="R" />
      </model>
      <model Id="Bzzzt" Name="{{M|Bzzzt}}">
        <tile Path="Creatures/sw_bzzzt.bmp" Foreground="y" DetailColor="W" />
      </model>
    </models>
  </module>
</embarkmodules>
