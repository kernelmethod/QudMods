<?xml version="1.0" encoding="utf-8" ?>
<objects>
  <!--
    ================================
    Helping hands modifications
    ================================

    * Make Helping Hands tinkerable.
  -->
  <object Name="Helping Hands" Load="Merge">
    <part Name="TinkerItem" Bits="0135" CanDisassemble="true" CanBuild="true" />
  </object>

  <!--
    ================================
    Fungal infections modifications
    ================================

    * Make fungal infections appear as physical features
  -->
  <object Name="FungalInfection" Load="Merge">
    <tag Name="ShowAsPhysicalFeature" />
  </object>
</objects>
